<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
    "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="requestsApprovalDAO" class="com._4s_.requestsApproval.dao.RequestsApprovalDAOHibernate"  init-method="init">
		<property name="sessionFactory" ref="sessionFactory"/>
		<property name="externalQueries" ref="requestsApprovalExternalQueries" />
	</bean>
	
	<bean id="requestsApprovalExternalQueries" class="com._4s_.requestsApproval.dao.ExternalQueries" scope="prototype">
		<property name="platformTransactionManager" ref ="transactionManager" />
	</bean>
</beans>