<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC
    "-//SPRING//DTD BEAN//EN"
    "http://www.springframework.org/dtd/spring-beans.dtd">

<beans default-autowire="no" default-lazy-init="false"
	>
	
	<bean id="religionBinder" class="com._4s_.attendance.web.binders.ReligionBinder">
	    <property name="baseManager">
			<ref bean="baseManager"/>
	    </property>
	</bean>

	<bean id="maritalStatusBinder" class="com._4s_.attendance.web.binders.MaritalStatusBinder">
	    <property name="baseManager">
			<ref bean="baseManager"/>
	    </property>
	</bean>
	
	
	<bean id="qualificationBinder" class="com._4s_.attendance.web.binders.QualificationBinder">
	    <property name="baseManager">
			<ref bean="baseManager"/>
	    </property>
	</bean>
	
	<bean id="departmentBinder" class="com._4s_.attendance.web.binders.AttendanceDepartmentBinder">
	    <property name="baseManager">
			<ref bean="baseManager"/>
	    </property>
	</bean>
	
	<bean id="titleBinder" class="com._4s_.attendance.web.binders.TitleBinder">
	    <property name="baseManager">
			<ref bean="baseManager"/>
	    </property>
	</bean>
	
	<bean id="workPeriodMasterBinder" class="com._4s_.attendance.web.binders.WorkPeriodMasterBinder">
	    <property name="baseManager">
			<ref bean="baseManager"/>
	    </property>
	</bean>
	
	<bean id="empBasicBinder" class="com._4s_.attendance.web.binders.EmpBasicBinder">
	    <property name="baseManager">
			<ref bean="baseManager"/>
	    </property>
	</bean>
	
</beans>