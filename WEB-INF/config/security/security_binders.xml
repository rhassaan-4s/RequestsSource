<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC
    "-//SPRING//DTD BEAN//EN"
    "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans default-autowire="no" default-lazy-init="false"
	 >


	<bean id="permissionBinder" parent="domainObjectBinder">
		<property name="bindedClass">
			<value>com._4s_.security.model.Permissions</value>
		</property>
		<property name="allowCreateNew">
			<value>false</value>
		</property>
	</bean>
	<bean id="roleBinder" parent="domainObjectBinder">
		<property name="bindedClass">
			<value>com._4s_.security.model.Roles</value>
		</property>
		<property name="allowCreateNew">
			<value>false</value>
		</property>
	</bean>
	<bean id="securityApplicationBinder" parent="domainObjectBinder">
		<property name="bindedClass">
			<value>com._4s_.security.model.SecurityApplication</value>
		</property>
		<property name="allowCreateNew">
			<value>false</value>
		</property>
	</bean>
	<bean id="userBinder" parent="domainObjectBinder">
		<property name="bindedClass">
			<value>com._4s_.security.model.User</value>
		</property>
		<property name="allowCreateNew">
			<value>false</value>
		</property>
	</bean>
</beans>