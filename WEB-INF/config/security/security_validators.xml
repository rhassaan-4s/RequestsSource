<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC
    "-//SPRING//DTD BEAN//EN"
    "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans
  default-autowire="no"
  default-lazy-init="false"
   >

<bean id="validateUser" class="com._4s_.security.web.validators.ValidateUser" > 
<property name="mgr">
		<ref bean="securityManager"/>
	</property>
</bean>

<bean id="validateForgetPassword" class="com._4s_.security.web.validators.ValidateForgetPassword" > 
	<property name="mgr">
		<ref bean="securityManager"/>
	</property>
</bean>

<bean id="validateRole" class="com._4s_.security.web.validators.ValidateRole" > 
	
</bean>


</beans>