<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
    "http://www.springframework.org/dtd/spring-beans.dtd">

<beans >
	<bean id="requestsApprovalDAO" class="com._4s_.requestsApproval.dao.RequestsApprovalDAOHibernate" >
		<property name="sessionFactory">
		      <ref bean="sessionFactory"/>
		</property>
		<property name="externalQueries">
			<ref bean="requestsApprovalExternalQueries" />
		</property>
	</bean>
	
	<bean id="requestsApprovalExternalQueries" class="com._4s_.requestsApproval.dao.ExternalQueries"
		singleton="false">
		<property name="platformTransactionManager">
			<ref bean="transactionManager" />
		</property>
	</bean>
</beans>